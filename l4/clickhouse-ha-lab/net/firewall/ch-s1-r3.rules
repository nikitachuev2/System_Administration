*filter
:INPUT DROP [0:0]
:FORWARD DROP [0:0]
:OUTPUT ACCEPT [0:0]

-A INPUT -i lo -j ACCEPT

-A INPUT -m conntrack --ctstate ESTABLISHED,RELATED -j ACCEPT

-A INPUT -p icmp -j ACCEPT

-A INPUT -p tcp --dport 22 -s 10.10.0.0/24 -j ACCEPT

-A INPUT -p tcp --dport 8123 -s 10.10.0.0/24 -j ACCEPT
-A INPUT -p tcp --dport 9000 -s 10.10.0.0/24 -j ACCEPT

COMMIT
